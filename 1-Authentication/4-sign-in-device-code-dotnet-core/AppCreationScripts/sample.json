{
    "Sample": {
        "Title": "An ASP.NET Core Web App which lets sign-in users on CIAM with work and school in my org",
        "Level": 200,
        "Client": "ASP.NET Core Web App",
        "Service": "Microsoft Graph",
        "RepositoryUrl": "ms-identity-ciam-device-code-dotnet-tutorial",
        "Endpoint": "AAD v2.0",
        "Platform": "DotnetCore",
        "Provider": "CIAM"
    },
    /*
    This section describes the Azure AD Applications to configure, and their dependencies
  */
    "AADApps": [
    {
      "Id": "service",
      "Name": "TodoListApi",
      "Kind": "WebApi",
      "HomePage": "https://localhost:44351",
      "Audience": "AzureADMyOrg",
      "SDK": "MicrosoftIdentityWeb",
      "ManualSteps": [],
      "Sample": {
        "SampleSubPath": "4-WebApp-Your-API\\4-1-MyOrg",
        "ProjectDirectory": "\\TodoListApi"
      },
      "OptionalClaims": {
        "AccessTokenClaims": [ "idtyp" ]
      },
      "Scopes": [ "ToDoList.Read", "ToDoList.ReadWrite" ],
      "AppRoles": [
        {
          "AllowedMemberTypes": [ "Application" ],
          "Name": "ToDoList.Read.All",
          "Description": "Allow application to read all ToDo list items"
        },
        {
          "AllowedMemberTypes": [ "Application" ],
          "Name": "ToDoList.ReadWrite.All",
          "Description": "Allow application to read and write into ToDo list"
        }
      ],
      "SampleSubPath": "2-Authorization\\1-call-own-api-aspnet-core-mvc",
      "ProjectDirectory": "\\TodoListApi"
    },
        {
            "Id": "webApp",
            "Name": "WebApp",
            "Kind": "Browserless",
            "Audience": "AzureADandPersonalMicrosoftAccount",
            "HomePage": "https://localhost:7274/",
            "ReplyUrls": "https://localhost:7274/, https://localhost:7274/signin-oidc",
            "LogoutUrl": "https://localhost:7274/signout-oidc",
            "SDK": "MicrosoftIdentityWeb",
            "RequiredResourcesAccess": [
                {
                    "Resource": "service",
                    "DelegatedPermissions": [
                        "ToDoList.Read",
                        "ToDoList.ReadWrite"
                    ]
                }
            ]
        }
    ],
    "CodeConfiguration": [
        {
            "App": "webApp",
            "SettingKind": "json",
            "SettingFile": "\\..\\appsettings.json",
            "Mappings": [
                {
                    "key": "ClientId",
                    "value": ".AppId"
                },
                {
                    "key": "TenantId",
                    "value": "$tenantId"
                }
            ]
        }
    ]
}